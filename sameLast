def main():
    userNumber()

def userNumber():
    number_1 = input("Please enter a number: ")
    number_2 = input("Please enter a number: ")
    number_3 = input("Please enter a number: ")

    if number_1[:-1] == number_2[:-1] == number_3[:-1]:
        print("True")
    else:
        print("False")
main()
