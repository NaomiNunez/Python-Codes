percentage = float(input("What is your percent grade?: "))
if percentage >= 90:
    print("You earned an 'A'")
elif percentage >= 80:
    print("You earned a 'B'")
elif percentage >= 70:
    print("You earned a 'C'")
elif percentage >= 60:
    print("You earned a 'D'")
elif percentage >= 50:
    print("You earned a 'F'")
else:
    print("YEEESH...that sucks.")
